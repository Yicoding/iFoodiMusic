<view>
  <view
    class="list"
    wx:for="{{wallList}}"
    wx:key="{{item.id}}"
    bindtap="goWallDetail"
    data-id="{{item.id}}">
    <image class="cover" src="{{item.coverImg}}"/>
    <view class="container">
      <text class="title">{{item.title}}</text>
      <text class="time">上传于 {{item.presentTime}}</text>
    </view>
  </view>
  <view class="info">{{info}}</view>
</view>