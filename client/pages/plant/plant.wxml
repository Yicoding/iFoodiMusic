<view>
  <view
    class="list"
    wx:for="{{wallList}}"
    wx:key="{{item.id}}"
    bindtap="goWallDetail"
    data-id="{{item.id}}">
    <image
      class="cover"
      src="{{item.coverImg}}"
      catchtap="viewImage"
      data-src="{{item.coverImg}}"
      data-urls="{{item.coverImg}}" />
    <view class="container">
      <text class="title">{{item.name}}</text>
      <text class="time">上传于 {{item.createTime}}</text>
    </view>
  </view>
  <view class="info">{{info}}</view>
</view>