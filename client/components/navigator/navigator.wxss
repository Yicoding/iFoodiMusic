
.navigator{
    position: fixed;
    bottom: 190rpx;
    right: 30rpx;
    z-index: 9;
}
.bg{
    height: 80rpx;
    background: #aaa;
    border-radius: 80rpx;
    display: flex;
    justify-content: flex-end;
    align-items: center;
    border: 5rpx solid #fff;
}
.container{
    width: 80rpx;
    height: 80rpx;
}
.song{
    margin: 0 15rpx 0 20rpx;
}
image{
    width: 100%;
    height: 100%;
}
.avatar{
    border-radius: 50%;
}
.close{
    width: 65rpx;
    height: 65rpx;
    position: absolute;
    top: -140rpx;
    right: 12.5rpx;
}
.state{
    width: 60rpx;
    height: 60rpx;
    position: absolute;
    top: -70rpx;
    right: 15rpx;
}